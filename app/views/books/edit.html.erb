 <%= form_with model:@book, local:true do |f| %>
    <div>
      <lavel for="title">Title</lavel><br />
     <%= f.text_field :title %>
    </div>
    <div>
      <lavel for="body">Body</lavel></br />
     <%= f.text_area :body %>
    </div>
    <div>
     <%= f.submit "Update book" %>
    </div>
    <% end %>
    <%= link_to "show",book_path(@book.id) %>
    <%= link_to "back","/books" %>